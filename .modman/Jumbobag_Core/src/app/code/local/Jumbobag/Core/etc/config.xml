<?xml version="1.0"?>
<config>
	<modules>
		<Jumbobag_Core>
			<version>1.3.1</version>
		</Jumbobag_Core>
	</modules>
	<global>
		<resources>
			<jumbobag_core_setup>
				<setup>
					<module>Jumbobag_Core</module>
					<class>Mage_Core_Model_Resource_Setup</class>
				</setup>
				<connection>
					<use>core_setup</use>
				</connection>
			</jumbobag_core_setup>
			<jumbobag_core_attributes>
				<setup>
					<module>Jumbobag_Core</module>
					<class>Mage_Eav_Model_Entity_Setup</class>
				</setup>
				<connection>
					<use>core_setup</use>
				</connection>
			</jumbobag_core_attributes>
		</resources>
		<helpers>
			<jumbobag_core>
				<class>Jumbobag_Core_Helper</class>
			</jumbobag_core>
		</helpers>
		<blocks>
			<jumbobag_core>
				<class>Jumbobag_Core_Block</class>
			</jumbobag_core>
			<popupbuilder>
				<rewrite>
					<PopupWidget>Jumbobag_Core_Block_PopupWidget</PopupWidget>
				</rewrite>
			</popupbuilder>
		</blocks>
	</global>
</config>
